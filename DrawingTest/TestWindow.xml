<?xml version="1.0" encoding="utf-8" ?>
<div style="justify-content: center; align-items: center;">
    <link rel="stylesheet" href="DrawingTest.styles.css" />
    <link rel="stylesheet" href="DrawingTest.TestWindow.css" />
    <style>
    .icon {
        border: 1 #0F0; /* short version */
    }
    </style>
    <template id="testbox">
        <div class="box">
            <header>
                {title}
            </header>
            <div>
                {children}
            </div>
        </div>
    </template>
    <template id="iconbox">
        <icon />
    </template>
    <div class="wrapper">
        <div class="red">Red</div>
        <div class="green">Green</div>
        <div class="blue">Blue</div>
    </div>
    <div class="wrapper" style="border-top: 0">
        <iconbox id="character-icon" icon-id="1" style="size: 40" enableMouse="true" />
        <iconbox icon-id="14" style="size: 40" />
        <uld-part uld="Achievement" part-list-id="4" part-index="5" style="size: 40" />
    </div>
    <clock format="HH:mm:ss" style="margin-top: 10" /> <!-- TODO: padding-top didn't work -->
    <span style="font: 24 Axis;">Axis</span>
    <span style="font: 24 Jupiter;">Jupiter</span>
    <span style="font: 24 Meidinger;">1234</span>
    <span style="font: 24 MeidingerMid;">5678</span>
    <span style="font: 24 TrumpGothic;">TrumpGothic</span>
    Some New<br/>Line Test
    <testbox id="sestringtest" title="SeString Test" />
</div>
